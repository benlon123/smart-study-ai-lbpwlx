
{
  "expo": {
    "name": "SmartStudy AI",
    "slug": "smartstudy-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/final_quest_240x240.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.smartstudyai",
      "buildNumber": "1",
      "icon": "./assets/images/final_quest_240x240.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleVersion": "1",
        "CFBundleShortVersionString": "1.0.0"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#000000"
      },
      "icon": "./assets/images/final_quest_240x240.png",
      "edgeToEdgeEnabled": true,
      "package": "com.yourname.smartstudyai",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "smartstudyai"
}
